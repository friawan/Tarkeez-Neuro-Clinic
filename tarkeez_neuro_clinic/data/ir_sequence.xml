<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="sequence_tarkeez_appointments" model="ir.sequence">
        <field name="name">appointments Sequence</field>
        <field name="code">tarkeez.appointments</field>
        <field name="prefix">AP/</field>
        <field name="padding">4</field>
        <field name="number_increment">1</field>
        <field name="number_next_actual">8</field>
        <field name="company_id" eval="False"/>
    </record>

    <record id="sequence_tarkeez_patient" model="ir.sequence">
        <field name="name">Patient Sequence</field>
        <field name="code">tarkeez.patient</field>
        <field name="prefix">PA/</field>
        <field name="padding">4</field>
        <field name="number_increment">1</field>
        <field name="number_next_actual">8</field>
        <field name="company_id" eval="False"/>
    </record>


</odoo>